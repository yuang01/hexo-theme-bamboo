<!-- 文章目录所需js -->
<%- partial('toc') %> 

<!-- 设置导航背景 -->
<%- partial('setHeader') %> 

<!-- 渲染issues标签里的内容 -->
<%- partial('issues') %>

<!-- 渲染远程json加载的图片标签(getPhotoOnline)里的内容 -->
<%- partial('getPhotoOnline') %>

<!-- 渲染远程json加载的talk标签(getTalkOnline)里的内容 -->
<%- partial('getTalkOnline') %>

<!-- 渲染远程json加载的site-card标签(getSiteOnline)里的内容 -->
<%- partial('getSiteOnline') %>

<!-- 输入框打字特效 -->
<%- partial('_partial/scripts/inputEffects') %>

<!-- markdown代码一键复制功能 -->
<%- partial('_partial/scripts/copy') %>

<!-- 图片懒加载 -->
<%- partial('_partial/scripts/lazyload') %>

<!-- 卡片滚动动画 -->
<% if(theme.scrollreveal.on) { %>
    <%- partial('_partial/scripts/scrollreveal') %>
<% } %>   

<!-- 评论所需js -->
<% if(page.comments !== false ) { %>
  <% if(theme.valine.on) { %>
    <!-- 弹幕所需的css和js -->
    <%- partial('_partial/scripts/danmu') %>
    <%- partial('_partial/comment/valine/script') %>
  <% } else if(theme.twikoo.on) { %>
    <%- partial('_partial/comment/twikoo/script') %>
  <% } else if(theme.gitment.on) { %>
    <%- partial('_partial/comment/gitment/script') %>
  <% } else if(theme.livere.on) {  %>
    <%- partial('_partial/comment/livere/script') %>
  <% } else if(theme.changyan.on) {  %>
    <%- partial('_partial/comment/changyan/script') %>
  <% } else if(theme.gitalk.on) {  %>
    <%- partial('_partial/comment/gitalk/script') %>
  <% } else if(theme.utterance.on) { %>
    <%- partial('_partial/comment/utterance/script') %>
  <% } else if(theme.waline.on) {  %>
    <%- partial('_partial/comment/waline/script') %>
  <% } else if(theme.beaudar.on) {  %>
    <%- partial('_partial/comment/beaudar/script') %>
  <% } %>
<% } %>

<!-- 鼠标点击特效 -->
<%- partial('_partial/scripts/cursor_effect') %>

<% if (theme.busuanzi.on) { %>
  <script async src="<%- url_for(theme.CDN.busuanzi) %>" data-pjax></script>
<% } %>

<!-- 轮播图标签 -->
<%- partial('_partial/scripts/swiperTag') %>